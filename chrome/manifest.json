{
  "manifest_version": 2,
  "name": "jatis - JavaDoc statistics",
  "description": "Find popular methods on top of each class (based on usage statistics)",
  "version": "0.1",
  "author": "CÃ©dric Reginster<cedric.reginster@gmail.com>",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://scg.unibe.ch/*"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["http://docs.oracle.com/javase/*"],
      "js" : [ "jquery-1.11.0.min.js","server.js", "template.js", "content.js"]
    }
  ],
  "page_action": {
    "default_icon": {
      "19": "icon16.png",
      "38": "icon48.png"
    },
    "default_title": "Jatis - Javadoc Statistics",
    "default_popup": "popup.html"
  }
}
